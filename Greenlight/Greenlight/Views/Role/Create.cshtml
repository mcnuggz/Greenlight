@model Microsoft.AspNet.Identity.EntityFramework.IdentityRole
@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>
@using (Html.BeginForm())
{
    <table id="tableRole">
        <tr>
            <td>Enter Role Name:</td>
            <td>@Html.EditorFor(m => m.Name)</td>
        </tr>
    </table>
    <input type="submit" value="Create Role" />
}

